<?xml version="1.0"?>

<block>
	<name>OFDM Parse MAC</name>
	<key>ieee802_11_ofdm_parse_mac</key>
	<category>IEEE802.11</category>
	<import>import ieee802_11</import>
	<make>ieee802_11.ofdm_parse_mac($debug)</make>

	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>

		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>

	<sink>
		<name>in</name>
		<type>message</type>
	</sink>

	<source>
		<name>out</name>
		<type>message</type>
        <optional>1</optional>
	</source>
</block>

